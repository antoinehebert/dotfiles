[alias]
        st = status
        co = checkout
        ci = commit
        br = branch
        last = log -1 --stat HEAD
        lg = log --oneline --graph --decorate --color
        lol = log --graph --decorate --color
        df = diff
        dff = diff --no-prefix -U10000
[push]
	default = simple
[diff]
	tool = meld
